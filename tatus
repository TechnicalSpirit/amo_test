[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Lev\Bizandsoft02\DataProvider;[m
[32m+[m
[32m+[m[32muse Faker\Factory;[m
[32m+[m
[32m+[m[32mclass ContactDataProvider[m
[32m+[m[32m{[m
[32m+[m[32m    /**[m
[32m+[m[32m     * @var \Faker\Generator[m
[32m+[m[32m     */[m
[32m+[m[32m    private static $faker;[m
[32m+[m[32m    public static function init():void[m
[32m+[m[32m    {[m
[32m+[m[32m        self::$faker = Factory::create();[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function makeFakeName():string[m
[32m+[m[32m    {[m
[32m+[m[32m        return self::$faker->name();[m
[32m+[m[32m    }[m
[32m+[m[32m    public static function makeFakePhone():array[m
[32m+[m[32m    {[m
[32m+[m[32m        $phoneEnum = ['WORK','HOME'];[m
[32m+[m[32m        return [[m
[32m+[m[32m            'value' => self::$faker->phoneNumber(),[m
[32m+[m[32m            'enum'  =>  $phoneEnum[array_rand($phoneEnum)][m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public static function makeFareEmail():array[m
[32m+[m[32m    {[m
[32m+[m[32m        $phoneEnum = ['WORK'];[m
[32m+[m[32m        return [[m
[32m+[m[32m            'value' => self::$faker->email(),[m
[32m+[m[32m            'enum'  =>  $phoneEnum[array_rand($phoneEnum)][m
[32m+[m[32m        ];[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
