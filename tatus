[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Lev\Bizandsoft02;[m
[32m+[m
[32m+[m[32muse AmoCRM\AmoContact;[m
[32m+[m
[32m+[m[32mclass Contact extends AmoContact[m
[32m+[m[32m{[m
[32m+[m[32m    public function __construct(array $data = [], $subdomain = null)[m
[32m+[m[32m    {[m
[32m+[m[32m        parent::__construct($data, $subdomain);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    public function createContact(array $phoneNumbers, array $emails): int|array[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->setCustomFields([m
[32m+[m[32m            [[m
[32m+[m[32m                '2211713' => $phoneNumbers,[m
[32m+[m[32m                '2211715' => $emails[m
[32m+[m[32m            ][m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        return $this->save();[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
