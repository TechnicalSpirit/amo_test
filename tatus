[32m+[m[32m<?php[m
[32m+[m
[32m+[m[32mnamespace Lev\Bizandsoft02\CLI\Commands;[m
[32m+[m
[32m+[m[32muse AmoCRM\AmoAPI;[m
[32m+[m[32muse Lev\Bizandsoft02\Contact;[m
[32m+[m[32muse Lev\Bizandsoft02\DataProvider\ContactDataProvider;[m
[32m+[m[32muse Symfony\Component\Console\Command\Command;[m
[32m+[m[32muse Symfony\Component\Console\Input\InputArgument;[m
[32m+[m[32muse Symfony\Component\Console\Input\InputInterface;[m
[32m+[m[32muse Symfony\Component\Console\Input\InputOption;[m
[32m+[m[32muse Symfony\Component\Console\Output\OutputInterface;[m
[32m+[m[32muse Symfony\Component\Console\Logger;[m
[32m+[m[32mclass CreateContacts extends Command[m
[32m+[m[32m{[m
[32m+[m[32m    protected static $defaultName = "create_contacts";[m
[32m+[m[32m    protected static $defaultDescription = "This command creates as many clients as you need";[m
[32m+[m
[32m+[m[32m    protected function configure(): void[m
[32m+[m[32m    {[m
[32m+[m[32m        $this->addOption("number",[m
[32m+[m[32m            null,[m
[32m+[m[32m        InputOption::VALUE_OPTIONAL,[m
[32m+[m[32m        'number of clients',[m
[32m+[m[32m        0);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    protected function execute(InputInterface $input, OutputInterface $output)[m
[32m+[m[32m    {[m
[32m+[m[32m        $nubmerOfContacts = $input->getOption("number");[m
[32m+[m
[32m+[m[32m        if($nubmerOfContacts > 0)[m
[32m+[m[32m            $this->activateAppAmoCRM($output);[m
[32m+[m
[32m+[m[32m        for($i = 0; $i < $nubmerOfContacts; $i++)[m
[32m+[m[32m        {[m
[32m+[m[32m            $contact = new Contact([[m
[32m+[m[32m                "name"                => ContactDataProvider::makeFakeName(),[m
[32m+[m[32m            ]);[m
[32m+[m[32m            $phoneNumbers = $this->createFakeData(2,fn () => ContactDataProvider::makeFakePhone());[m
[32m+[m[32m            $emails = $this->createFakeData(2,fn() => ContactDataProvider::makeFareEmail());[m
[32m+[m
[32m+[m[32m            $id = $contact->createContact($phoneNumbers,$emails);[m
[32m+[m
[32m+[m[32m            $output->writeln("Contact with ID ($id) has been created");[m
[32m+[m[32m        }[m
[32m+[m[32m        $output->writeln("[END OF COMMANDS] $nubmerOfContacts contacts created");[m
[32m+[m
[32m+[m[32m        return 0;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private function activateAppAmoCRM(OutputInterface $output): void[m
[32m+[m[32m    {[m
[32m+[m[32m        $output->writeln("start connect to amoCRM");[m
[32m+[m
[32m+[m[32m        AmoAPI::oAuth2([m
[32m+[m[32m            $_ENV['BASE_DOMAIN'],[m
[32m+[m[32m            $_ENV['CLIENT_ID'],[m
[32m+[m[32m            $_ENV['CLIENT_SECRET'],[m
[32m+[m[32m            $_ENV['REDIRECT_URL'],[m
[32m+[m[32m        );[m
[32m+[m
[32m+[m[32m        $output->writeln("The connection was successful");[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    private function createFakeData(int $items,callable $dataProvider): array[m
[32m+[m[32m    {[m
[32m+[m[32m        $res = [];[m
[32m+[m[32m        for($i = 0;$i < $items;$i++)[m
[32m+[m[32m        {[m
[32m+[m[32m            $res[] = $dataProvider();[m
[32m+[m[32m        }[m
[32m+[m[32m        return $res;[m
[32m+[m[32m    }[m
[32m+[m[32m}[m
